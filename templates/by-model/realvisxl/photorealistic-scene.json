{
  "id": "realvisxl-photorealistic-scene",
  "name": "RealVisXL Photorealistic",
  "version": "1.0.0",
  "description": "Ultra-realistic photography with RealVisXL. Exceptional for product shots, environments, and lifelike character renders.",
  "author": "RawDiffusion",
  "license": "MIT",
  "tags": ["photorealistic", "realistic", "photography", "product", "environment", "sdxl"],
  "difficulty": "beginner",
  "category": "txt2img",
  "preview": "portrait.svg",

  "requirements": {
    "base_model": {
      "architecture": "sdxl",
      "recommended": ["realvisxl_v5.safetensors", "realvisxl_v4.safetensors"],
      "vram_minimum_gb": 8
    }
  },

  "parameters": {
    "prompt": {
      "type": "string",
      "default": "professional product photography, studio lighting, white background",
      "label": "Scene Description",
      "description": "Describe your photorealistic scene",
      "group": "basic"
    },
    "negative_prompt": {
      "type": "string",
      "default": "cartoon, anime, illustration, painting, drawing, blurry, low quality, oversaturated, text, watermark",
      "label": "Negative Prompt",
      "group": "basic"
    },
    "style": {
      "type": "select",
      "default": "studio",
      "label": "Photography Style",
      "options": ["studio", "natural", "cinematic", "commercial", "editorial"],
      "group": "style"
    },
    "lighting": {
      "type": "select",
      "default": "soft",
      "label": "Lighting",
      "options": ["soft", "dramatic", "natural", "rim", "golden_hour"],
      "group": "style"
    },
    "width": {
      "type": "integer",
      "default": 1024,
      "min": 512,
      "max": 2048,
      "step": 64,
      "group": "advanced"
    },
    "height": {
      "type": "integer",
      "default": 1024,
      "min": 512,
      "max": 2048,
      "step": 64,
      "group": "advanced"
    },
    "steps": {
      "type": "integer",
      "default": 30,
      "min": 20,
      "max": 60,
      "group": "advanced"
    },
    "cfg_scale": {
      "type": "number",
      "default": 7.0,
      "min": 1,
      "max": 15,
      "step": 0.5,
      "group": "advanced"
    },
    "seed": {
      "type": "integer",
      "default": -1,
      "label": "Seed (-1 = random)",
      "group": "advanced"
    }
  },

  "pipeline": [
    {
      "id": "load_model",
      "type": "load_model",
      "config": {
        "model_type": "sdxl",
        "scheduler": "DPM++ 2M Karras"
      }
    },
    {
      "id": "build_prompt",
      "type": "build_prompt",
      "config": {
        "style_templates": {
          "studio": "{prompt}, studio photography, soft box lighting, clean background",
          "natural": "{prompt}, natural lighting, outdoor photography",
          "cinematic": "{prompt}, cinematic lighting, film grain, anamorphic",
          "commercial": "{prompt}, commercial photography, advertising, high end",
          "editorial": "{prompt}, editorial photography, magazine quality, artistic"
        }
      }
    },
    {
      "id": "generate",
      "type": "generate",
      "config": {
        "enable_freeu": true,
        "freeu_params": {
          "s1": 0.9,
          "s2": 0.2,
          "b1": 1.3,
          "b2": 1.4
        }
      },
      "outputs": ["latents"]
    },
    {
      "id": "decode",
      "type": "decode"
    },
    {
      "id": "save",
      "type": "save",
      "config": {
        "format": "png",
        "quality": 95
      }
    }
  ],

  "presets": {
    "product_shot": {
      "prompt": "professional product photography, studio lighting, white background, commercial quality",
      "style": "commercial",
      "lighting": "soft",
      "cfg_scale": 7.5,
      "description": "Clean product photography"
    },
    "portrait": {
      "prompt": "professional headshot, person, studio lighting, shallow depth of field",
      "style": "studio",
      "lighting": "soft",
      "cfg_scale": 7.0,
      "description": "Professional portrait"
    },
    "environment": {
      "prompt": "beautiful landscape, golden hour, photorealistic",
      "style": "natural",
      "lighting": "golden_hour",
      "cfg_scale": 6.5,
      "description": "Natural environment"
    }
  },

  "app_adaptations": {
    "vscode": {
      "generate_standalone": true
    },
    "davinci": {
      "timeline_integration": false,
      "notes": "Great for still frames and thumbnails"
    },
    "gimp": {
      "layer_output": true
    }
  }
}
