{
  "name": "rawdiffusion",
  "displayName": "RawDiffusion",
  "description": "Browse like Midjourney. Code like a developer. Own your workflow.",
  "version": "0.1.0",
  "publisher": "rawdiffusion",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Machine Learning",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "stable diffusion",
    "diffusers",
    "comfyui",
    "ai",
    "image generation",
    "machine learning"
  ],
  "activationEvents": [
    "onView:rawdiffusion.gallery",
    "onCommand:rawdiffusion.openGallery"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rawdiffusion.openGallery",
        "title": "Open Gallery",
        "category": "RawDiffusion",
        "icon": "$(preview)"
      },
      {
        "command": "rawdiffusion.generateCode",
        "title": "Generate Python Code",
        "category": "RawDiffusion",
        "icon": "$(code)"
      },
      {
        "command": "rawdiffusion.runWorkflow",
        "title": "Run Workflow",
        "category": "RawDiffusion",
        "icon": "$(play)"
      },
      {
        "command": "rawdiffusion.importComfyUI",
        "title": "Import ComfyUI Workflow",
        "category": "RawDiffusion",
        "icon": "$(folder-opened)"
      },
      {
        "command": "rawdiffusion.configureModels",
        "title": "Configure Model Paths",
        "category": "RawDiffusion",
        "icon": "$(gear)"
      },
      {
        "command": "rawdiffusion.refreshGallery",
        "title": "Refresh Gallery",
        "category": "RawDiffusion",
        "icon": "$(refresh)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rawdiffusion",
          "title": "RawDiffusion",
          "icon": "media/icons/rawdiffusion.svg"
        }
      ]
    },
    "views": {
      "rawdiffusion": [
        {
          "type": "webview",
          "id": "rawdiffusion.gallery",
          "name": "Template Gallery"
        },
        {
          "type": "webview",
          "id": "rawdiffusion.output",
          "name": "Output"
        },
        {
          "id": "rawdiffusion.recent",
          "name": "Recent Generations"
        },
        {
          "id": "rawdiffusion.models",
          "name": "Available Models"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "rawdiffusion.importComfyUI",
          "when": "view == rawdiffusion.gallery",
          "group": "navigation"
        },
        {
          "command": "rawdiffusion.refreshGallery",
          "when": "view == rawdiffusion.gallery",
          "group": "navigation"
        },
        {
          "command": "rawdiffusion.configureModels",
          "when": "view == rawdiffusion.models",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "rawdiffusion.runWorkflow",
          "when": "resourceFilename =~ /.*\\.py$/",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "RawDiffusion",
      "properties": {
        "rawdiffusion.modelPaths": {
          "type": "array",
          "default": [],
          "description": "Paths to search for model files (.safetensors, .ckpt)",
          "items": {
            "type": "string"
          }
        },
        "rawdiffusion.pythonPath": {
          "type": "string",
          "default": "python3",
          "description": "Path to Python interpreter with diffusers installed"
        },
        "rawdiffusion.outputDirectory": {
          "type": "string",
          "default": "./outputs",
          "description": "Directory for generated images"
        },
        "rawdiffusion.defaultDevice": {
          "type": "string",
          "enum": ["auto", "cuda", "cpu", "mps"],
          "default": "auto",
          "description": "Default device for inference"
        },
        "rawdiffusion.enableFreeU": {
          "type": "boolean",
          "default": true,
          "description": "Enable FreeU quality enhancement by default"
        },
        "rawdiffusion.enableMemoryOptimization": {
          "type": "boolean",
          "default": true,
          "description": "Enable CPU offloading for lower VRAM usage"
        },
        "rawdiffusion.templateDirectory": {
          "type": "string",
          "default": "",
          "description": "Custom directory for additional templates"
        },
        "rawdiffusion.stabilityApiKey": {
          "type": "string",
          "default": "",
          "description": "Stability AI API key (for cloud generation)"
        },
        "rawdiffusion.replicateApiKey": {
          "type": "string",
          "default": "",
          "description": "Replicate API key (for cloud generation)"
        },
        "rawdiffusion.davinciMediaPath": {
          "type": "string",
          "default": "",
          "description": "DaVinci Resolve Media Pool folder for push integration"
        },
        "rawdiffusion.gimpPath": {
          "type": "string",
          "default": "gimp",
          "description": "Path to GIMP executable"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "eslint": "^8.x",
    "typescript": "^5.x",
    "@vscode/vsce": "^2.x"
  },
  "dependencies": {
    "glob": "^10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolveythorax-pixel/rawdiffusion"
  },
  "license": "MIT"
}
